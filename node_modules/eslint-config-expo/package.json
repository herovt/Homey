  "author": "Expo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/expo/eslint-config-expo/issues"
  },
  "homepage": "https://github.com/expo/eslint-config-expo#readme",
  "dependencies": {
    "babel-eslint": "^7.2.3",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.1.0"
  },
  "devDependencies": {
    "@expo/spawn-async": "^1.3.0",
    "eslint": "^4.4.1",
